{
  "name": "JIRA-GITHUB-BRIDGE-EXT",
  "description": "Jump to JIRA from github commits",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],

  "action": {
    "default_popup": "popup.html"
  }
}
